                        <span class="k">for</span><span class="p">(</span><span class="kt">int</span> <span class="n">a</span><span class="o">=</span><span class="mi">0</span><span class="p">;</span> <span class="n">a</span><span class="o">&lt;</span><span class="mi">8</span><span class="p">;</span> <span class="n">a</span><span class="o">++</span><span class="p">){</span>
