        <span class="kd">const</span> <span class="p">{</span> <span class="nx">pushHotels</span><span class="p">,</span> <span class="nx">clickedHotels</span> <span class="p">}</span> <span class="o">=</span> <span class="nx">useContext</span><span class="p">(</span><span class="nx">RecentContext</span><span class="p">);</span>
    
        <span class="kd">const</span> <span class="nx">handleClickHotel</span> <span class="o">=</span> <span class="p">(</span><span class="nx">hotelName</span><span class="p">,</span> <span class="nx">e</span><span class="p">)</span> <span class="o">=&gt;</span> <span class="p">{</span>
            <span class="nx">alert</span><span class="p">(</span><span class="s2">`</span><span class="p">${</span><span class="nx">hotelName</span><span class="p">}</span><span class="s2"> ì„ ì¡°íšŒí•˜ì˜€ìŠµë‹ˆë‹¤.`</span><span class="p">)</span>
            <span class="nx">pushHotels</span><span class="p">(</span><span class="nx">hotelName</span><span class="p">)</span>
        <span class="p">}</span>
    
        <span class="k">return</span> <span class="p">(</span>
            <span class="o">&lt;</span><span class="nx">Container</span> <span class="nx">onClick</span><span class="o">=</span><span class="p">{</span><span class="nx">handleClickHotel</span><span class="p">.</span><span class="nx">bind</span><span class="p">(</span><span class="k">this</span><span class="p">,</span> <span class="nx">info</span><span class="p">.</span><span class="nx">name</span><span class="p">)}</span><span class="o">&gt;</span>
                <span class="o">&lt;</span><span class="nx">Column</span><span class="o">&gt;</span>
                    <span class="o">&lt;</span><span class="nx">HotelImage</span> <span class="nx">img</span><span class="o">=</span><span class="p">{</span><span class="nx">info</span><span class="p">.</span><span class="nx">imageUrl</span><span class="p">}</span> <span class="sr">/</span><span class="err">&gt;
</span>                <span class="o">&lt;</span><span class="sr">/Column</span><span class="err">&gt;
</span>                <span class="o">&lt;</span><span class="nx">Column2</span><span class="o">&gt;</span>
                    <span class="o">&lt;</span><span class="nx">HotelInfo</span><span class="o">&gt;</span>
                        <span class="o">&lt;</span><span class="nx">Row</span><span class="o">&gt;</span>
                            <span class="o">&lt;</span><span class="nx">Name</span><span class="o">&gt;</span><span class="p">{</span><span class="nx">info</span><span class="p">.</span><span class="nx">name</span><span class="p">}</span><span class="o">&lt;</span><span class="sr">/Name</span><span class="err">&gt;
