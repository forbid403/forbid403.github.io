</span>    
                        <span class="o">&lt;</span><span class="nx">Facilities</span><span class="o">&gt;</span>
                            <span class="p">{</span><span class="nx">info</span><span class="p">.</span><span class="nx">freeServices</span><span class="p">.</span><span class="nx">map</span><span class="p">((</span><span class="nx">service</span><span class="p">,</span> <span class="nx">idx</span><span class="p">)</span> <span class="o">=&gt;</span> <span class="o">&lt;</span><span class="nx">Service</span> <span class="nx">key</span><span class="o">=</span><span class="p">{</span><span class="nx">idx</span><span class="p">}</span><span class="o">&gt;</span><span class="p">{</span><span class="nx">service</span><span class="p">}</span><span class="o">&lt;</span><span class="sr">/Service&gt;</span><span class="se">)</span><span class="err">}
